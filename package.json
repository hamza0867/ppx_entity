{
  "name": "ppx_entity",
  "version": "0.0.0",
  "description": "My Project",
  "esy": {
    "build": "dune build -p <PACKAGE_NAME>",
    "buildDev": "pesy"
  },
  "buildDirs": {
    "library": {
      "require": "ppxlib",
      "namespace": "PpxEntity",
      "name": "ppx_entity.lib",
      "kind": "ppx_rewriter",
      "preprocess": [
        "pps",
        "ppxlib.metaquot"
      ]
    }
  },
  "scripts": {
    "pesy": "bash -c 'env PESY_MODE=update pesy'"
  },
  "dependencies": {
    "@esy-ocaml/reason": "*",
    "@opam/dune": "2.6.0",
    "@opam/ppxlib": "0.14.0",
    "ocaml": "4.10.0"
  },
  "devDependencies": {
    "ocaml": "4.10.0",
    "refmterr": "*",
    "pesy": "*",
    "@opam/merlin": "*",
    "@opam/ocaml-lsp-server": "ocaml/ocaml-lsp:ocaml-lsp-server.opam"
  }
}

